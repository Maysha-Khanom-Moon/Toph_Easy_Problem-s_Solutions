#include <bits/stdc++.h>

using namespace std;

int main()
{
    string a,b;
    cin>>a>>b;

    map<char ,int> m;
    map<char ,int> n;

    if(a.size()==b.size()){

        int l = a.size(); ///l=length
        for(int i=0; i<l; i++){
            m[a[i]]++;
        }
        for(int i=0; i<l; i++){
            n[b[i]]++;
        }

        if(m==n) cout<<"Yes"<<endl;
        else cout<<"No"<<endl;
    }
    else{
        cout<<"No"<<endl;
    }

	return 0;
}
